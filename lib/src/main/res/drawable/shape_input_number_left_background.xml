<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/color_f5" />
            <corners android:bottomLeftRadius="@dimen/in_dp_4" android:bottomRightRadius="0dp" android:topLeftRadius="@dimen/in_dp_4" android:topRightRadius="0dp" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <ripple android:color="?attr/colorControlHighlight" tools:ignore="NewApi">
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="#F5F5F5" />
                    <corners android:bottomLeftRadius="@dimen/in_dp_4" android:bottomRightRadius="0dp" android:topLeftRadius="@dimen/in_dp_4" android:topRightRadius="0dp" />
                </shape>
            </item>
        </ripple>
    </item>
</selector>